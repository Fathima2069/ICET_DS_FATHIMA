#include <stdio.h>
void main(){
 int day;
 printf("Enter tha no: \n");
 scanf("%d",&day);
 switch(day){
 case 1:printf("Sun");
        break;
 case 2:printf("mon");
        break;
 case 3:printf("tue");
        break;
 case 4:printf("wed");
        break;
 case 5:printf("thu");
        break;
 case 6:printf("fri");
        break;
 case 7:printf("sat");
        break;
 dafault:printf("Sun");
 }
}
